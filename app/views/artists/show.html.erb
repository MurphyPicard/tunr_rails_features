<h2>
  <%= @artist.name %>
  <%= link_to "(edit)", edit_artist_path(@artist) %>
</h2>
<h4><%= @artist.nationality %></h4>

<img class='artist-photo' src="<%= @artist.photo_url %>">

<h2><%= @artist.name %>'s Songs</h2>

<ul>
<% @artist.songs.each do |song|  %>
  <li><%= link_to song.title, artist_song_path(@artist, song) %></li>
<% end  %>
</ul>

<h2>Add a new Song for <%= @artist.name %></h2>

<%= render 'songs/form' %>
